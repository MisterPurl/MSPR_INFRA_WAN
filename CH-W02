conf t
ip vrf INTERNE
rd 100:100
route-target export 100:100
route-target import 100:100
end
!
conf t
ip prefix-list EIGRP permit 10.203.0.0/24
ip prefix-list EIGRP permit 10.207.0.0/24
ip prefix-list EIGRP permit 10.210.0.0/24
route-map BGP_EIGRP deny 10
match ip address prefix-list EIGRP
route-map BGP_EIGRP permit 20
end
!
conf t
router eigrp 100
network 10.98.220.4 0.0.0.1
network 10.98.220.6 0.0.0.1
redistribute bgp 5000 metric 1500 100 255 1 1500
address-family ipv4 vrf INTERNE
redistribute bgp 5000 metric 1500 100 255 1 1500
network 10.98.220.4 0.0.0.1
network 10.98.220.6 0.0.0.1
autonomous-system 5000
exit-address-family
end
!
conf t 
router ospf 100
network 10.98.200.6 0.0.0.1 area 0
network 10.98.200.8 0.0.0.1 area 0
network 10.98.200.4 0.0.0.1 area 0
network 10.98.199.4 0.0.0.0 area 0
end
!
conf t 
router bgp 5000
redistribute connected
redistribute eigrp 100
neighbor 10.98.199.1 remote-as 5000
neighbor 10.98.199.3 remote-as 5000
neighbor 10.98.199.2 remote-as 5000
neighbor 10.98.230.7 remote-as 1836
neighbor 10.98.230.5 remote-as 1836
address-family ipv4 vrf INTERNE
redistribute connected
redistribute eigrp 100
exit-address-family
no auto-summary
end